{
  "name": "v-money",
  "version": "0.8.3",
  "description": "Vue currency input/directive mask",
  "main": "dist/v-money.js",
  "scripts": {
    "size": "size-limit",
    "upgrade:check": "ncu",
    "upgrade:apply": "ncu -a",
    "dev": "poi --serve ./src/docs/index.js",
    "build": "poi --prod --no-html",
    "docs:build": "poi --prod ./src/docs/index.js -d ./docs/",
    "docs:serve": "open http://localhost:5000/docs/ && serve ./",
    "compile": "npm run build -- --disable-compress",
    "test": "jest"
  },
  "keywords": [
    "vue",
    "input",
    "mask",
    "directive",
    "currency",
    "money"
  ],
  "files": [
    "dist/"
  ],
  "size-limit": [
    {
      "path": "dist/v-money.js",
      "limit": "1.6 KB"
    }
  ],
  "author": "Marcos Neves <marcos.neves@gmail.com> (https://vuejs-tips.github.io/)",
