{
  "name": "v-money",
  "version": "0.8.2",
  "description": "Vue currency input/directive mask",
  "main": "dist/v-money.js",
  "scripts": {
    "size": "size-limit",
    "upgrade:check": "ncu",
    "upgrade:apply": "ncu -a",
    "dev": "poi --serve ./src/docs/docs.vue --host 0.0.0.0",
    "build": "poi --prod",
    "docs:build": "poi --prod ./src/docs/docs.vue --dist ./docs/",
    "docs:serve": "open http://localhost:5000/docs/ && serve ./",
    "compile": "npm run build -- --disable-compress",
    "test": "jest"
  },
  "keywords": [
    "vue",
    "input",
    "mask",
    "directive",
    "currency",
    "money"
  ],
  "files": [
    "dist/"
  ],
  "size-limit": [
    {
      "path": "dist/v-money.js",
      "limit": "1.6 KB"
    }
  ],
  "author": "Marcos Neves <marcos.neves@gmail.com> (https://vuejs-tips.github.io/)",
  "license": "MIT",
  "devDependencies": {
    "babel-jest": "^24.8.0",
    "jest": "^24.8.0",
    "npm-check-updates": "^3.1.10",
    "poi": "^12.6.10",
    "size-limit": "^1.3.6",
    "vue-template-compiler": "^2.6.10",
    "vuetify": "^1.5.14",
    "@babel/core": "^7.4.5",
    "babel-loader": "^8.0.6",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-stage-2": "^7.0.0",
    "@babel/register": "^7.4.4"
  }
}
